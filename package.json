{
    "name": "zendesk-messaging-client",
    "version": "1.0.0",
    "description": "Zendesk messaging API client",
    "keywords": [
        "zendesk",
        "messaging",
        "api",
        "rest",
        "client",
        "sunshine"
    ],
    "homepage": "https://github.com/LarisLab/zendesk-messaging-client",
    "bugs": {
        "url": "https://github.com/LarisLab/zendesk-messaging-client/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LarisLab/zendesk-messaging-client.git"
    },
    "license": "MIT",
    "author": "Laris Lab",
    "sideEffects": false,
    "type": "module",
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./types": "./dist/types.js",
        "./sdk": "./dist/sdk.js"
    },
    "source": "./src/index.ts",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && typedoc src/index.ts",
        "generate": "openapi-ts"
    },
    "browserslist": [
        "defaults"
    ],
    "prettier": {
        "semi": false,
        "singleQuote": true
    },
    "release": {
        "branches": [
            "+([0-9])?(.{+([0-9]),x}).x",
            "main",
            "next",
            "next-major",
            {
                "name": "beta",
                "prerelease": true
            },
            {
                "name": "alpha",
                "prerelease": true
            }
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/git",
            "@semantic-release/github"
        ]
    },
    "devDependencies": {
        "@hey-api/client-fetch": "^0.13.1",
        "@hey-api/openapi-ts": "^0.74.0",
        "prettier": "^3.5.3",
        "typedoc": "^0.28.5",
        "typescript": "^5.8.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {}
}